<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox prefHeight="300.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.simpolab.client_elector.login.LoginController">
    <children>
        <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="303.0" prefWidth="588.0" VBox.vgrow="ALWAYS">
            <children>
                <Label alignment="CENTER" layoutX="133.0" layoutY="110.0" style="&#10;" text="Username" textAlignment="CENTER" textFill="#9f9f9f" wrapText="false">
                    <font>
                        <Font size="18.0" />
                    </font>
                </Label>
                <Button fx:id="btnLogin" layoutX="267.0" layoutY="234.0" mnemonicParsing="false" onAction="#onLoginClick" text="Login">
               <font>
                  <Font size="18.0" />
               </font></Button>
                <TextField fx:id="txtUsername" layoutX="336.0" layoutY="109.0" />
                <Label alignment="CENTER" layoutX="135.0" layoutY="164.0" style="&#10;" text="Password" textAlignment="CENTER" textFill="#9f9f9f" wrapText="false">
                    <font>
                        <Font size="18.0" />
                    </font>
                </Label>
                <PasswordField fx:id="txtPassword" layoutX="336.0" layoutY="163.0" />
            <Label layoutX="163.0" layoutY="33.0" text="Login sistema votazioni">
               <font>
                  <Font size="28.0" />
               </font>
            </Label>
            </children>
        </AnchorPane>
    </children>
</VBox>
